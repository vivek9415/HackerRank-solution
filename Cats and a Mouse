#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() 
{
int a[5],n=3,i,dh,r,j,t,di,x;
scanf("%d",&x);
for(int i=0;i<x;i++)
{
   for(int j=0;j<n;j++)
 {
     scanf("%d",&a[j]);
 }
 t=(a[2]-a[0]);
 dh=abs(t);
 r=(a[2]-a[1]);
 di=abs(r);
if((dh)>(di))
 {
     printf("Cat B\n");
 }
 if((dh)<(di))
 {
     printf("Cat A\n");
 }
 if((dh)==(di))
 {
     printf("Mouse C\n");
 }
}
}
