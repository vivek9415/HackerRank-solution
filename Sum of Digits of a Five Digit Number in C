#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main()
{
    int n,vi=0,sum=0;
    scanf("%d",&n);
    if(n>=10000 && n<=99999) 
             {
                 while(n>0)
             {
             vi = n%10; 
              sum=sum+vi;
             n=n/10;
            }
        printf("%d\n",sum);
    }
    return 0;
    }
